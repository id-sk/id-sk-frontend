    "govuk-frontend-config": "*",
    "govuk-frontend-lib": "*",
    "highlight.js": "^11.8.0",
    "iframe-resizer": "^4.3.6",
    "js-beautify": "^1.14.7",
    "marked": "^5.0.2",
    "nunjucks": "^3.2.4",
    "outdent": "^0.8.0",
    "shuffle-seed": "^1.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "cheerio": "^1.0.0-rc.12",
    "cssnano": "^6.0.1",
    "govuk-frontend-tasks": "*",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "nodemon": "^2.0.22",
    "postcss-pseudo-classes": "^0.2.1",
    "puppeteer": "^20.4.0",
    "rollup": "^3.23.0",
    "sassdoc": "^2.7.4",
